(version 2.0.0~alpha4)

(build
 (progn
  (when
   %{pkg-self:dev}
   (run dune subst))
  (run dune build -p %{pkg-self:name} -j %{jobs} @install)))

(depends dune ocaml uutf uucp uunf dune-build-info)

